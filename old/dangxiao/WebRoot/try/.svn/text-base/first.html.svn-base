		<input type = "button" name = "myname" value = "提交" onclick="javascript:alert('你成功了')"></input>
		<input type = "text" name = "test0" value = "ff" onchange = "alert(haha)"></input>
		<input type = "text" name = "test1" value = "ff" onfocus = "alert(haha)"></input>
	</form>>
	<select name = "book">
