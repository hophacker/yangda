<title>无标题文档</title>
<script language="JavaScript">
		function loadform(){
			alert("开始自动装载!");
		}
		function unloadform(){
			alert("开始卸载!");
		}
		function change(){
			document.write("Event has been activated!!");
		}
	</script>
</head>

<body> <!-- onload="loadform()" onunload="unloadform()"> -->
